{
  "name": "tof-builds",
  "author": "dvaji",
  "version": "0.11.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "clean:dist": "rimraf dist/",
    "build:types": "tsc src/index.ts --declarationDir dist/ --lib esnext --emitDeclarationOnly --declaration",
    "build:lib": "vite build",
    "build": "pnpm clean:dist && pnpm build:lib && pnpm build:types",
    "lint": "eslint src/ --ext .ts",
    "generate": "node ./scripts/gen/index.mjs",
    "format-data": "prettier --loglevel warn --write \"src/data/**/*.json\"",
    "prepublishOnly": "pnpm generate && pnpm build"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.405.0",
    "@types/node": "^18.17.14",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "colorette": "^2.0.20",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "fastest-levenshtein": "^1.0.16",
    "fs-extra": "^11.1.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  }
}
