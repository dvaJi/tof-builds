{
  "name": "@dvaji/tof-builds",
  "author": "dvaji",
  "version": "0.7.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "license": "",
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "prepare": "npm run build",
    "clean:dist": "rimraf dist/",
    "build:types": "tsc src/index.ts --declarationDir dist/ --lib esnext --emitDeclarationOnly --declaration",
    "build:lib": "vite build",
    "build": "npm run clean:dist && npm run build:lib && npm run build:types",
    "generate": "node ./scripts/gen/index.mjs",
    "check-missing": "npm run generate && npm run build && node ./scripts/check-missing.js",
    "format-data": "prettier --loglevel warn --write \"src/data/**/*.json\"",
    "gen": "node scripts/gen/index.mjs",
    "prepublishOnly": "npm run generate && npm run build"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "aws-sdk": "^2.1255.0",
    "chalk": "^5.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "globby": "^13.1.2",
    "jsdom": "^21.1.0",
    "node-fetch": "^3.3.0",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.4"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "eslint": "^8.34.0"
  }
}
